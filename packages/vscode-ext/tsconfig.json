{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2015",
    "lib": ["ES2022"],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["node", "@types/express-serve-static-core", "@types/cors"]
  },
  "include": ["src/**/*"]
}
